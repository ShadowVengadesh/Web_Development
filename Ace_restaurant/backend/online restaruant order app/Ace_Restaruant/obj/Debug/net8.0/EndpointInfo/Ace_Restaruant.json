{
  "openapi": "3.0.1",
  "info": {
    "title": "Ace_Restaruant",
    "version": "1.0"
  },
  "paths": {
    "/api/Food/Item created": {
      "post": {
        "tags": [
          "Food"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AceRestaurant"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/AceRestaurant"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/AceRestaurant"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Food/Get all food items": {
      "get": {
        "tags": [
          "Food"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "AceRestaurant": {
        "required": [
          "isAvailable",
          "name",
          "price"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "minLength": 1,
            "type": "string"
          },
          "cuisineType": {
            "type": "string",
            "nullable": true
          },
          "price": {
            "type": "number",
            "format": "double"
          },
          "description": {
            "type": "string",
            "nullable": true
          },
          "outTime": {
            "type": "string",
            "format": "date-time"
          },
          "isAvailable": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      }
    }
  }
}